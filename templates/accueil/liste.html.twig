				<div class="card card-warning card-outline" style=" cursor: pointer;">
					<div class="card-body">
						<div class="row no-gutters align-items-center">
							<div class="col mr-2">
								<div class="text-xs font-weight-bold text-warning text-uppercase mb-1"><a href="{{path('inter_liste', {'id':entreprise.id })}}" style="color: black;">{{entreprise.nom}}</a></div>
								<div class="h5 mb-0 font-weight-bold text-gray-800"></div>
							</div>
							<div class="col-auto">
							</div>
						</div>
					</div>
				</div>
			</div> 
			
          

		{% endfor %}
	</div>
{% endblock %}
